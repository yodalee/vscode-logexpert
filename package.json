{
  "name": "logexpert",
  "displayName": "LogExpert",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "logexpert.OpenFile",
        "title": "Open File in LogExpert"
      },
      {
        "command": "logexpert.AddFilter",
        "title": "Add Filter"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "logexpert-panel",
          "title": "LogExpert",
          "icon": ""
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "logexpert.AddFilter",
          "when": "view == logexpert-filters",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "logexpert-panel": [
        {
          "id": "logexpert-filters",
          "name": "filters"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}